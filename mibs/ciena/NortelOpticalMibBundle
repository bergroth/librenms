GccValues ::= INTEGER {
	    unknown(0),
        none(1),
		gcc0(2),
		gcc1(3),
		gcc2(4)
}


-- The Admin State and Primary States 
-- are doubly defined in the 40G Fac
-- module and the OME6500 Equipment
-- module. Ultimately, they need to 
-- be merged. In the meantime, they
-- need to be concurrently updated.
AdminState ::= INTEGER  {
    unknown(0),
    is(1),
    oos(2)
}

PrimaryState ::= INTEGER {
    unknown(0),
    is(1),
    is-anr(2),
    oos-au(3),
    oos-ma(4),
    oos-auma(5),
    oos-maanr(6)
}

LoopbackType ::= INTEGER {
    unknown(0),
    none(1),
    facility(2),
    terminal(3),
    efmremote(4)
}

Status ::= INTEGER {
    unknown(0),
    enabled(1),
    disabled(2)
}

FecFormat ::= INTEGER {
    unknown(0),
    off(1),
    rs8(2),
    scfec(3),
    bch20(4),
    pfec(5)
}


